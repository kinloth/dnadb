<div class="container">
  <div class="row">
    <h3>Genes</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @genes.each do |gene| %>
            <tr>
              <%= render gene %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
